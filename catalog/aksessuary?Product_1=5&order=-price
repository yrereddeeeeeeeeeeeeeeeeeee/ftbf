    <script>
        $(function(){
            $('.sidebar-catalog-menu a.has-subitems').on('click', function(e){
                e.preventDefault();
                $(this).closest('li.has-subitems').toggleClass('toggled');
                return false;
            });

            var $priceSlider = $('#price-slider');
            if ($priceSlider.length) {
                $priceSlider.filterSlider({
                    from: "FilterForm_price_from",
                    to: "FilterForm_price_to",
                    min: 10,
                    max: 49900,
                    name: "price",
                    step: 10
                });
            }

            var $impedanceSlider = $('#impedance-slider');
            if ($impedanceSlider.length) {
                $impedanceSlider.filterSlider({
                    from: "FilterForm_impedance_from",
                    to: "FilterForm_impedance_to",
                    min: 0,
                    max: 0,
                    name: "impedance",
                    step: 1
                });
            }

            var loading;

            function loadProducts(){
                var data = $('#filter-form').serialize();
                var order = $('.sorting a.active').data('current');

                data = data || '';
                if (order) {
                    data += '&order=' + order;
                }

                $('.goods-products-loader').addClass('loading');
                $.ajax({
                    'url': "/catalog/aksessuary",
                    'data': data,
                    'success': function(html){
                        $('#products-block').replaceWith($(html).find('#products-block'));
                    }
                })
            }

            $('#filter-form input').on('change', function(){
                clearTimeout(loading);
                loading = setTimeout(function(){
                    loadProducts();
                }, 300);
            });

            $(document).on('change', '.nav-select', function (e) {
                e.preventDefault();
                var val = $(this).val();
                window.location = val;
                return false;
            });

            if (window.location.hash == '#openedFilter') {
              $('.category-filter-toggle').click();
            }
        })
    </script>


</body>
</html>
