    <script>
        $(function(){
            $('.msticky').psticky();

            $('.related-slider').slick({
                accessibility: false,
                autoplay: true,
                autoplaySpeed: 6000,
                  slidesToShow: 4,
                  slidesToScroll: 4,
              responsive: [
                {
                  breakpoint: 1250,
                  settings: {
                    slidesToShow: 3,
                    slidesToScroll: 3
                  }
                },
                {
                  breakpoint: 1008,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1
                  }
                }
              ]
            });

            $('.form-block .title').on('click', function(){
                $('.form-block form').toggleClass('hide');
            });

            var $frama = $('.f-rama');
            if ($(window).width() >= 1250) {
                $frama.attr('data-width', "634");
                $frama.data('width', '634');

              $frama.attr('data-height', "480");
              $frama.data('height', '480');
            } else if ($(window).width() > 1008) {
              $frama.attr('data-width', "474");
              $frama.data('width', '474');
            }

            $frama.fotorama();

            function setType()
            {
                if ($('input[name="price"]:checked').length) {
                    var val = $('input[name="price"]:checked').val();
					var $label = $('label[for="price-' + val + '"]');
                    var name = $label.find('.name').text().trim();
					var avail = $label.find('.avail-holder').text().trim();
					if (avail != 'В наличии') {
						name += ' (' + avail + ')';
					}
					$('#FastOrderForm_type_raw').val(val);
                    $('#FastOrderForm_type').val(name);
                }
            }

            $('input[name="price"]').on('change', function(){
                setType();
            });

            setType();

			
        })
    </script>


</body>
</html>
