<script>
	var reg_popup = $('#reg_popup'),
		reg_popup_timeout = 0;
	function initRegPopup()
	{
		$('#reg_popup_close, #zanaves').on('click', function () {
			reg_popup.hide();
			$('#zanaves').hide();
			if (typeof yaCounter19694461 != "undefined") {
				yaCounter19694461.reachGoal('popup_down');
			}
		});
		$('#reg_popup_submit_reg_popup').on('click', function () {
			if (!$("#reg_popup form").valid()) {
				$('.reg_popup_formgroup:first').addClass('mrg0T');
				return false;
			}
			if (typeof yaCounter19694461 != "undefined") {
				yaCounter19694461.reachGoal('popup_reg');
			}
			var data = {};
			reg_popup.find('input').each(function (index, el) {
				var name = $(el).attr('name'),
					value = $(el).val();
				data[name] = value;
			});
			$.post('/bitrix/components/allfordj/reg.popup/ajax.php', data, function (result) {
				if (result.SUCCESS == 'y') {
					$('#reg_popup .popup.show').children().not('#reg_popup_close, #reg_popup_success').hide();
					$('#reg_popup_success').show();
					reg_popup.height(250);
					$('#reg_popup_close, #zanaves').click(function () {
						window.location.href = window.location.href;
					});
				} else {
					if (result.ERROR) {
						if (result.ERROR == 'email_exist') {
							window.email_exist = $('#reg_popup_email').val();
							$('#reg_popup_close').trigger('click');
							$('.avtorization-call').trigger('click');
						} else {
							alert(result.ERROR);
						}
					}
				}
			}, 'json');
		});
		if (reg_popup_timeout > 0)
			setTimeout(function () {
				if ($.cookie('STOP_REG_POPUPwerwrwer') == undefined) {
					//SetCookie('STOP_REG_POPUPwerwrwer', 'Y', 365);
					$.cookie('STOP_REG_POPUPwerwrwer', 1, {
						expires: 365,
						path: '/'
					});
					reg_popup.show();
					$('#zanaves').show();
					if (typeof yaCounter19694461 != "undefined") {
						yaCounter19694461.reachGoal('popup_up');
					}
				}
			}, reg_popup_timeout);
	}
	function initRegPopupBtn()
	{
		$('.product_discount_reg').addClass('pointer').on('click', function () {
			reg_popup.show();
			$('#zanaves').show();
			//SetCookie('STOP_REG_POPUPwerwrwer', 'Y', 365);
			$.cookie('STOP_REG_POPUPwerwrwer', 1, {expires: 365, path: '/'});
			if (typeof yaCounter19694461 != "undefined") {
				yaCounter19694461.reachGoal('popup_salesticker');
			}
		});
	}
	function initRegPopupValidate()
	{
		$("#reg_popup form").validate({
			onkeyup: false,
			focusCleanup: true,
			rules: {
				reg_popup_email: {email: true, required: true},
				reg_popup_name: {required: true},
				licenses_popup_OCB: {required: true}
			}
		});
	}

	BX.ready(function () {
		initRegPopup();
	});
	initRegPopupValidate();
	initRegPopupBtn();
</script>
